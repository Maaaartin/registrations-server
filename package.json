{
  "name": "registrations-next",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maaaartin/registrations-server.git"
  },
  "scripts": {
    "prisma:generate": "rm -rf prisma/client/sql && sh entrypoint.sh prisma generate --sql",
    "prisma:pull": "sh entrypoint.sh prisma db pull --force",
    "dev": "NODE_OPTIONS=--enable-source-maps sh entrypoint.sh yarn next dev",
    "build": "NODE_OPTIONS=--enable-source-maps sh entrypoint.sh yarn next build",
    "start": "NODE_OPTIONS=--enable-source-maps sh entrypoint.sh yarn next start",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build:image": "sh build-image.sh",
    "postinstall": "next telemetry disable && DATABASE_URL=\"file:dev.db\" yarn prisma generate --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/lab": "^7.0.0",
    "@mui/material": "^7.3.6",
    "@mui/system": "^7.3.6",
    "@mui/x-charts": "8",
    "@mui/x-data-grid": "8",
    "@mui/x-date-pickers": "8",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7",
    "csv-stringify": "^6.6.0",
    "luxon": "^3.7.2",
    "next": "16",
    "pg": "^8.16.3",
    "prisma": "7",
    "ramda": "^0.32.0",
    "react": "19",
    "react-dom": "19",
    "redis": "^5.10.0",
    "swr": "^2.3.8",
    "zod": "4"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^22",
    "@types/pg": "^8.16.0",
    "@types/ramda": "^0.31.1",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@eslint/eslintrc": "^3.1.0",
    "eslint": "9.39.2",
    "eslint-config-next": "15.5.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "5.8.2"
  }
}
