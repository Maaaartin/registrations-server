    "@mui/system": "7",
    "@mui/x-charts": "8",
    "@mui/x-data-grid": "8",
    "@mui/x-date-pickers": "8",
    "@prisma/client": "^6.19.1",
    "csv-stringify": "^6.6.0",
    "luxon": "^3.7.2",
    "next": "15.5.9",
    "prisma": "^6.19.1",
    "ramda": "^0.32.0",
    "react": "19",
    "react-dom": "19",
    "redis": "^5.10.0",
    "swr": "^2.3.8",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
