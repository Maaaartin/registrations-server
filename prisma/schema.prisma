generator client {
  provider        = "prisma-client-js"
  output          = "./client"
  previewFeatures = ["typedSql"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model imports {
  id          Int       @id @default(autoincrement())
  pcv         BigInt?
  country     String?
  import_date DateTime? @db.Date
}

