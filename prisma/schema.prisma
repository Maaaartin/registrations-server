generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["typedSql"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This model contains an expression index which requires additional setup for migrations. Visit https://pris.ly/d/expression-indexes for more info.
model registrations {
  id                                                              Int       @id @default(autoincrement())
  datum_1_registrace                                              DateTime? @db.Date
  datum_1_registrace_v_cr                                         DateTime? @db.Date
  ztp                                                             String?
  es_eu                                                           String?   @map("es/eu")
  druh_vozidla                                                    String?
  druh_vozidla_2_radek                                            String?
  kategorie_vozidla                                               String?
  tovarni_znacka                                                  String?
  typ                                                             String?
  varianta                                                        String?
  verze                                                           String?
  vin                                                             String?
  obchodni_oznaceni                                               String?
  vyrobce_vozidla                                                 String?
  vyrobce_motoru                                                  String?
  typ_motoru                                                      String?
  max_vykon                                                       Float?    @db.Real
  max_vykon_otacky                                                Float?    @db.Real
  palivo                                                          String?
  zdvihovy_objem                                                  Float?    @db.Real
  plne_elektricke_vozidlo                                         Boolean?
  hybridni_vozidlo                                                Boolean?
  trida_hybridniho_vozidla                                        String?
  emisni_limit                                                    String?
  stupe__plneni_emisni_urovne                                     String?   @map("stupeň_plneni_emisni_urovne")
  korigovany_soucinitel_absorpce                                  Float?    @db.Real
  co2                                                             Float?    @db.Real
  co2_mesto                                                       Float?    @db.Real
  co2_mimo_mesto                                                  Float?    @db.Real
  specificke_co2                                                  Float?    @db.Real
  snizeni_emisi_nedc                                              Float?    @db.Real
  snizeni_emisi_wltp                                              Float?    @db.Real
  spotreba_mesto                                                  Float?    @db.Real
  spotreba_mimo_mesto                                             Float?    @db.Real
  spotreba_kombinovana                                            Float?    @db.Real
  spotreba_pri_rychlosti                                          String?
  spotreba_el_mobil                                               Int?
  dojezd_zr                                                       Int?
  vyrobce_karoserie                                               String?
  druh_karoserie                                                  String?
  vyrobni_cislo_karoserie                                         String?
  barva                                                           String?
  barva_doplnkova                                                 String?
  pocet_mist_celkem                                               Int?
  pocet_mist_k_sezeni                                             Int?
  pocet_mist_k_stani                                              Int?
  delka                                                           Int?
  sirka                                                           Int?
  vyska                                                           Int?
  rozvor                                                          Int?
  rozchod                                                         Int?
  provozni_hmotnost                                               Int?
  nejvetsi_technicky_pripustna_hmotnost                           Int?
  nejvetsi_povolena_hmotnost                                      Int?
  nejvetsi_technicky_pripustna_hmotnost_na_napravu                String?
  nejvetsi_povolena_hmotnost_na_napravu                           String?
  nejvetsi_technicky_pripustna_hmotnost_pripojneho_vozidla_brzden Int?
  nejvetsi_povolena_hmotnost_pripojneho_vozidla_brzdeneho         Int?
  nejvetsi_technicky_pripustna_hmotnost_pripojneho_vozidla_ne_brz Int?
  nejvetsi_povolena_hmotnost_pripojneho_vozidla_ne_brzdeneho      Int?
  nejvetsi_technicky_pripustna_hmotnost_jizdni_soupravy           Int?
  nejvetsi_povolena_hmotnost_jizdni_soupravy                      Int?
  hmotnosti_vozidla_pri_testu_wltp                                String?
  predpis_spotreba_paliva                                         String?
  prumerna_hodnota_uzitecneho_zatizeni                            String?
  spojovaci_zarizeni                                              String?
  pocet_naprav                                                    Int?
  naprav_pohanenych                                               String?
  kola_a_pneumatiky_naprava_1                                     String?
  kola_a_pneumatiky_naprava_2                                     String?
  kola_a_pneumatiky_naprava_3                                     String?
  kola_a_pneumatiky_naprava_4                                     String?
  vnejsi_hluk_vozidla_stojici                                     Float?    @db.Real
  vnejsi_hluk_vozidla_otacky                                      Float?    @db.Real
  vnejsi_hluk_vozidla_jizda                                       Float?    @db.Real
  nejvyssi_rychlost                                               Float?    @db.Real
  pomer_vykon_hmotnost                                            Float?    @db.Real
  inovativni_technologie                                          String?
  stupe__dokonceni                                                String?   @map("stupeň_dokonceni")
  faktor_odchylky_de                                              Float?    @db.Real
  faktor_verifikace_vf                                            Int?
  ucel_vozidla                                                    String?
  dalsi_zaznamy                                                   String?
  alternativni_provedeni                                          String?
  cislo_tp                                                        String?
  cislo_orv                                                       String?
  druh_rz                                                         String?
  zarazeni_vozidla                                                String?
  status                                                          String?
  pcv                                                             Int?
  abs                                                             Boolean?
  airbag                                                          String?
  asr                                                             Boolean?
  brzdy_nouzova                                                   Boolean?
  brzdy_odlehcovaci                                               Boolean?
  brzdy_parkovaci                                                 Boolean?
  brzdy_provozni                                                  Boolean?
  dopl_kovy_text_na_tp                                            String?   @map("doplňkovy_text_na_tp")
  hmotnosti_provozni_do                                           Int?
  hmotnosti_zatizeni_sz                                           Int?
  hmotnosti_zatizeni_sz_typ                                       String?
  hydropohon                                                      Boolean?
  objem_cisterny                                                  Float?    @db.Real
  zatizeni_strechy                                                Int?
  cislo_motoru                                                    String?
  nejvyssi_rychlost_omezeni                                       Int?
  ovladani_brzd_sz                                                String?
  ovladani_brzd_sz_druh                                           String?
  retarder                                                        Boolean?
  rok_vyroby                                                      Int?
  delka_do                                                        Int?
  lozna_delka                                                     Int?
  lozna_sirka                                                     Int?
  vyska_do                                                        Int?
  typ_kod                                                         String?
  rm_zaniku                                                       String?

  @@index([datum_1_registrace], map: "idx_registrations_year")
  @@index([barva])
  @@index([id])
  @@index([tovarni_znacka])
}
